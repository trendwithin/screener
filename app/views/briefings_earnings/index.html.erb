<div class="container">
  <div class="row">
    <div class="col-md-4">
      <table class="table table-striped briefings_earnings">
        <tr>
          <th>Symbol:</th>
          <th>Earnings </th>
          <th>Expectation</th>
          <th>Year Ago Earnings</th>
          <th>Revenue</th>
          <th>Release Date</th>
        </tr>
        <% @briefings_earnings.each do |e| %>
          <tr>
            <td><%= e.symbol %></td>
            <td><%= e.earnings %></td>
            <td><%= e.expectation %></td>
            <td><%= e.year_ago_earnings %></td>
            <td><%= e.revenue %></td>
            <td><%= e.saved_on %></td>
          </tr>
        <% end %>
        <table>
    </div>
  </div>
</div>
